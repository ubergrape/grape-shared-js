    "lib"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "color": "^0.10.1",
    "css-functions": "^1.0.5",
    "css-vendor": "^0.3.8",
    "grape-js-emoji": "^0.0.1",
    "grape-theme": "3.x",
    "jss": "^9.8.0",
    "jss-default-unit": "^8.0.0",
    "jss-expand": "^5.0.0",
    "jss-extend": "^6.2.0",
    "jss-increase-specificity": "^0.3.5",
    "jss-isolate": "^5.0.0",
    "jss-props-sort": "^6.0.0",
    "jss-vendor-prefixer": "^7.0.0",
    "lodash": "^3.10.0",
    "material-ui": "1.0.0-beta.22",
    "mdurl": "^1.0.1",
    "normalize-jss": "^3.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-jss": "^8.3.3",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "reset-jss": "^0.1.0",
    "styled-jss": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cpy": "^5.0.0",
    "es5-shim": "^4.1.10",
    "es6-shim": "^0.32.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-ubergrape": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "expect.js": "^0.3.1",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "karma": "^0.13.8",
    "karma-chrome-launcher": "^0.1.10",
    "karma-cli": "^0.0.4",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^2.0.3",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.5.1",
    "lint-staged": "^7.2.0",
    "mocha": "^5.0.4",
    "node-libs-browser": "^0.5.2",
    "prettier": "^1.13.5",
    "react-addons-test-utils": "^0.14.0",
    "react-styleguidist": "^5.1.1",
    "svgo": "^0.6.0",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  },
  "scripts": {
    "all": "yarn lint && yarn test && yarn build",
    "clean": "rm -fr ./lib/*",
    "build": "yarn clean && yarn svg && yarn build:lib",
    "build:lib": "babel src --out-dir lib",
    "build:lib:watch": "babel src --out-dir lib --watch",
    "start": "NODE_ENV=development webpack-dev-server --hot",
    "lint": "eslint ./src --fix",
    "precommit": "lint-staged",
    "svg": "yarn svg:clean && yarn svg:copy && yarn svg:export",
    "svg:clean": "svgo -f src/svg-icons/svg -o src/svg-icons/svg --enable removeTitle",
    "svg:copy": "cpy src/svg-icons/svg/*.svg lib/svg-icons/svg",
    "svg:export": "babel-node ./bin/generateIconsIndex.js",
    "test": "karma start",
    "test:watch": "NODE_ENV=test karma start",
    "sg": "styleguidist server",
    "prepublishOnly": "yarn all"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
